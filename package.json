{
  "name": "@justinbeckwith/disco",
  "version": "0.0.1",
  "description": "Download Google API discover docs and do stuff with them",
  "main": "./build/src/index.js",
  "bin": {
    "disco": "./build/src/cli.js"
  },
  "types": "./build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/JustinBeckwith/disco.git"
  },
  "scripts": {
    "check": "gts check",
    "clean": "gts clean",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "fix": "gts fix",
    "compile": "tsc -p .",
    "test": "nyc mocha build/test -r source-map-support/register",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "start": "npm run compile && node build/src/index.js"
  },
  "keywords": [
    "google",
    "api"
  ],
  "author": {
    "name": "Justin Beckwith"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "ora": "^2.0.0",
    "progress": "^2.0.0",
    "retry-axios": "^0.3.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "@types/nock": "^9.1.3",
    "@types/node": "^9.6.5",
    "@types/ora": "^1.3.4",
    "@types/progress": "^2.0.1",
    "codecov": "^3.0.0",
    "gts": "^0.5.3",
    "mocha": "^5.1.0",
    "nock": "^9.2.5",
    "nyc": "^11.4.1",
    "source-map-support": "^0.5.3",
    "typescript": "~2.8.1"
  },
  "files": [
    "build/src",
    "LICENSE",
    "packge.json",
    "README.md"
  ],
  "nyc": {
    "exclude": [
      "build/test"
    ]
  }
}
